<script type="text/javascript">

$(document).ready(function(){

<% @strips.each_with_index do |strip, a| %>

<% strip.projects.each_with_index do |project, b| %>

<% if strip.category == "narrow" %>

$("#narrowphoto_<%= a %><%= b %> a img").hover(
   
   
   function(){
     $("div.category_<%= a %>").html('<h2 class="boldfirstnarrow"><%= project.first_name %></h2><h2 class="lastnarrow"><%= project.last_name %></h2><h4 class="yearnarrow"><%= project.yearcompleted %></h4><h4 class="locationnarrow"><%= project.location %></h4>');
   },
   function(){
     $("div.category_<%= a %>").html('<h2 class="namenarrow"><%= strip.name %></h2>');
   }
  );

<% elsif strip.category == "medium" %>
   
$("#normalphoto_<%= a %><%= b %> a img").hover(
  

   function(){
     $("div.category_<%= a %>").html('<h2 class="boldfirstnormal"><%= project.first_name %></h2><h2 class="lastnormal"><%= project.last_name %></h2><h4 class="yearnormal"><%= project.yearcompleted %></h4><h4 class="locationnormal"><%= project.location %></h4>');
   }, 
  function(){
     $("div.category_<%= a %>").html('<h2 class="namenormal"><%= strip.name %></h2>');
   }
  
  );

<% else %>
   
$("#broadphoto_<%= a %><%= b %> a img").hover(
   
   function(){
     $("div.category_<%= a %>").html('<h2 class="boldfirstbroad"><%= project.first_name %></h2><h2 class="lastbroad"><%= project.last_name %></h2><h4 class="yearbroad"><%= project.yearcompleted %></h4><h4 class="locationbroad"><%= project.location %></h4>');
   },
   function(){
     $("div.category_<%= a %>").html('<h2 class="namebroad"><%= strip.name %></h2>');
   }
  );

<% end %>

<% end %>

<% end %>


});

</script>

<% end %>

