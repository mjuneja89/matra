$('#images').html("<%=j render(partial: 'shared/image', collection: @images) %>");
$('#arrowbox1').html("<%=j render(partial: 'shared/arrowleft') %>");

<% if @images.last_page? %>
  $('#arrowbox').hide();
<% else %>
  $('#arrowbox').show();
  $('#arrowbox').html("<%=j render(partial: 'shared/arrowright') %>");
<% end %>


