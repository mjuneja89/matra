<div class="matra-row" style="margin-top: 3%">
	<div class="name-project">
		<div class="ProjectNameBox">
			<h3 class="ProjectName"><%= project.name %></h3>
		</div>
	</div>
	<div class="photo-project">
		<div class="ProjectPhotoBox">
			<% if project.images.first.square.present? %>
		    <%= link_to strip_project_path(project.strip, project) do %>
		     	<%= image_tag project.images.first.square.photo_url, :class => "fitimg" %>
			<% end %>
		   <% end %>	
		   <% if project.images.first.landscape.present? %>
		    <%= link_to strip_project_path(project.strip, project) do %> 
		     <%= image_tag project.images.first.landscape.photo_url, :class => "fitimg" %>
		   <% end %> 
		   <% end %> 
		   <% if project.images.first.portrait.present? %>
		    <%= link_to strip_project_path(project.strip, project) do %>
             <%= image_tag project.images.first.portrait.photo_url, :class => "fitimg" %>
            <% end %> 
		   <% end %>  
		</div>
	</div>
</div>


