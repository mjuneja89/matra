$('#awards').html("<%=j render(partial: 'shared/award', collection: @awards) %>");
$('#arrowbox1').html("<%=j render(partial: 'arrowleft') %>");

<% if @awards.last_page? %>
  $('#arrowbox').hide();
<% else %>
  $('#arrowbox').show();
  $('#arrowbox').html("<%=j render(partial: 'arrowright') %>");
<% end %>


