$('#images').html("<%=j render(partial: 'image', collection: @images) %>");
$('#arrowbox1').html("<%=j render(partial: 'arrowleft') %>");

<% if @images.last_page? %>
  $('#arrowbox').hide();
<% else %>
  $('#arrowbox').show();
  $('#arrowbox').html("<%=j render(partial: 'arrowright') %>");
<% end %>


