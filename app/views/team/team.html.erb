<html>

<% content_for :head do %>
	  
  <title>matra architects: our team</title>

  <script type="text/javascript">
  
  $(document).ready(function(){
    
   <% @users.each_with_index do |user, i| %>
    
    <% unless user.role == "admin" %>  
     
     $(".user_<%= i %>").hover(function(){
    		$(".categorybroad").html("<h2 class="namebroad" style="font-family:"Swis721 Blk2 BT W05 Black"
        "><%= user.first_name %></h2>"), function(){
    	$(".categorybroad").html("<h2 class="namebroad">team</h2>")  
     });

    <% end %> 

   <% end %> 

  });
     
  </script>

<% end %>

<body>

<div class="container-fluid" style="margin-top: 3%">
  
  <%= render :partial => 'strip' %>
  
  <div style="display: none"> 
   <%= paginate @users, :remote => true %>
  </div> 
  
</div>

</div>


</body>

</html>
